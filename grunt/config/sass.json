{
	"dist": {
		"options": {
			"style": "expanded",
			"compass": false,
			"sourcemap": "none",
			"includePaths": [
				"node_modules/"
			]
		},
		"files": [{
			"expand": true,
			"cwd": "<%= paths.webRoot %>/scss",
			"src": ["*.scss"],
			"dest": "<%= paths.webRoot %>/css",
			"ext": ".css"
		}]
	}
}
